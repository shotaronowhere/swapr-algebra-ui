  "scripts": {
    "compile-contract-types": "yarn compile-external-abi-types && yarn compile-v3-contract-types",
    "compile-external-abi-types": "typechain --target ethers-v6 --out-dir src/abis/types './src/abis/**/*[!dbg].json'",
    "compile-v3-contract-types": "typechain --target ethers-v6 --out-dir src/types/v3 './node_modules/@uniswap/v3-core/artifacts/contracts/**/*[!dbg].json'",
    "build": "yarn i18n:extract && yarn i18n:compile && react-scripts build",
    "build:simple": "react-scripts build",
    "i18n:extract": "node --max-old-space-size=8192 node_modules/.bin/lingui extract --locale en-US",
    "i18n:compile": "lingui compile",
    "integration-test": "start-server-and-test 'serve build -l 3000' http://localhost:3000 'cypress run --record'",
    "graphql:generate": "graphql-codegen --config codegen.yml",
    "postinstall": "yarn compile-contract-types",
    "start-with-graphql": "yarn graphql:generate && react-scripts start --host 0.0.0.0",
    "start": "react-scripts start --host 0.0.0.0",
    "test": "react-scripts test --env=./custom-test-env.js",
    "reinstall": "rm -rf node_modules && yarn"
  },
  "eslintConfig": {
    "extends": "react-app",
    "ignorePatterns": [
